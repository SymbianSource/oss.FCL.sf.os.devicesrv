RESOURCE SSM_START_PROCESS_INFO r_cmd_mylocationsengine
    {
    priority = 0x7EEC;
    name = "z:\\sys\\bin\\mylocationsengine.exe";
    execution_behaviour = ESsmFireAndForget;
    }

