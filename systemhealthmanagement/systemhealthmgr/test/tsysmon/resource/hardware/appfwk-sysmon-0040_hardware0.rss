	{
	severity = ECmdCriticalSeverity;
	}



RESOURCE SSM_START_PROCESS_INFO r_command_app_restartfail
	{
	name = "tsysmon_app_restartfail.exe";
	args = "APPFWK-SYSMON-0040";
	}


