RESOURCE SSM_START_AMA_STARTER r_command_ama_starter
	{
	severity = ECmdIgnoreFailure;
	dsc_id = 0x2000836D;		// removing this id will cause a rcomp failure unless dsc_id is #defined in ssmcmd.rh file.
	}

