START_TESTCASE DEVSRVS-SHMA-SYSMON-0021
RUN_PROGRAM 100 setcap tsysmon_app_register.exe 100 sysmontest_register_protserv.exe
RUN_TEST_STEP 100 tsysmon_server Register z:\shmatest\configs\tsysmon.ini APPFWK-SYSMON-0021
END_TESTCASE DEVSRVS-SHMA-SYSMON-0021
